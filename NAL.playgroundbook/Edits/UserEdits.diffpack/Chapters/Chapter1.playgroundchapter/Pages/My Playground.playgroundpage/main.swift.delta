let nars = NARS()

let robin: Term = .word("robin")
let bird: Term = .word("bird")
let animal: Term = .word("animal")
let mammal: Term = .word("mammal")
    
var history = [String]()
nars.output = { s in
    history.append(s)
    print(s)
}

nars.process(
    ("bird" --&gt; "animal")-*,
    (robin --&gt; bird)-*,
    (bird --&gt; animal)-?,
    (bird --&gt; "mammal")-?,
    (bird --&gt; mammal)-*(0, 0.9),
    (bird --&gt; mammal)-?
)

debugPrint(nars.memory)

nars.process(
    ("bird" --&gt; "?")-?,
    ("?" --&gt; mammal)-?,
    ("?" --&gt; "?")-?
)


