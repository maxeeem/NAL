			<string>/*
 * each rock has a functionality
 * @author Nicknames
 */

let nars = NARS() // Quine ( translates between Swift and Narsese )

let robin = Term.word("robin")
let bird = Term.word("bird")
let animal = Term.word("animal")
let mammal = Term.word("mammal")
    
var history = [String]()
nars.output = { s in
    history.append(s)
    print(s)
}

nars.process(
    ("bird"   --&gt;     "animal"  )-*, //(1, .9)
    (robin    --&gt;     bird      )-*,
    (bird     --&gt;     animal    )-?,
    (bird     --&gt;     "mammal"  )-?,
    (bird     --&gt;     mammal    ) -* (0, 0.9),
    (bird     --&gt;     mammal    )-?
)

nars.process(
    ("bird" --&gt; "?")-?,
    ("?" --&gt; mammal)-?,
    ("?" --&gt; "?")-?
)

debugPrint(nars.memory)

